{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "excludePackagePatterns": [
        "^org\.springframework\.boot",
        "^org\.springframework",
        "^org\.apache\.tomcat",
        "^commons\-fileupload"
      ],
      "enabled": false
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": false
  }
}
