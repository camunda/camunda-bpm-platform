{
  "version": "7.22.0-SNAPSHOT",
  "name": "camunda-external-task-client-js",
  "exports": "./index.js",
  "type": "module",
  "repository": "https://github.com/camunda/camunda-bpm-platform.git",
  "author": "Camunda Services GmbH",
  "license": "Apache-2.0",
  "scripts": {
    "license-check": "node ./scripts/licenseCheck.cjs",
    "pretest": "node_modules/.bin/eslint  lib index.js",
    "test": "jest lib --verbose --coverage --no-color && node test/license-check.js",
    "test:watch": "jest lib --watch --verbose --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@babel/preset-env": "^7.24.5",
    "@jest/globals": "^29.7.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-bpmn-io": "1.0.0",
    "eslint-plugin-camunda-licensed": "0.4.6",
    "eslint-plugin-jest": "^28.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "form-data": "^4.0.0",
    "jest": "29.7.0",
    "license-checker": "^25.0.1",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "got": "^14.2.1"
  },
  "description": "Implement your [BPMN Service Task](https://docs.camunda.org/manual/latest/user-guide/process-engine/external-tasks/) in NodeJS.",
  "bugs": {
    "url": "https://github.com/camunda/camunda-bpm-platform/issues"
  },
  "homepage": "https://github.com/camunda/camunda-bpm-platform/clients/nodejs#readme",
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(got|chalk)).+\\.js$"
    ],
    "moduleNameMapper": {
      "#(.*)": "<rootDir>/node_modules/$1"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
