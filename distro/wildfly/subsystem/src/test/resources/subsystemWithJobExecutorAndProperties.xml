<subsystem xmlns="urn:org.camunda.bpm.jboss:1.1">
  <job-executor>
    <core-threads>3</core-threads>
    <max-threads>5</max-threads>
    <queue-length>10</queue-length>
    <job-acquisitions>
      <job-acquisition name="default">
      </job-acquisition>
      <job-acquisition name="anders">
        <properties>
          <property name="lockTimeInMillis" value="600000"/>
          <property name="waitTimeInMillis" value="10000"/>
          <property name="maxJobsPerAcquisition" value="5"/>
        </properties>
      </job-acquisition>
      <job-acquisition name="mixed">
        <properties>
          <property name="lockTimeInMillis" value="500000"/>
        </properties>
      </job-acquisition>
    </job-acquisitions>
  </job-executor>
</subsystem>
