<?xml version="1.0" encoding="UTF-8"?>
<definitions id="taskAssigneeExample"
	xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
	targetNamespace="Examples">

	<process id="testConversationalBeanStoreFlush" isExecutable="true">

		<startEvent id="startEvent" />
		
		<serviceTask id="serviceTask" camunda:delegateExpression="#{setProcessScopedMessageDelegate}" />

		<userTask id="wait" />

		<endEvent id="endEvent" />

		<sequenceFlow targetRef="serviceTask" sourceRef="startEvent" />	
		<sequenceFlow targetRef="wait" sourceRef="serviceTask" />
		<sequenceFlow targetRef="endEvent" sourceRef="wait" />

	</process>
</definitions>