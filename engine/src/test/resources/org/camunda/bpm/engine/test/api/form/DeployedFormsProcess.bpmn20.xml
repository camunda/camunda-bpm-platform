<?xml version="1.0" encoding="UTF-8" ?>

<definitions id="definitions"
             targetNamespace="http://camunda.org/schema/1.0/bpmn20" 
             xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:camunda="http://camunda.org/schema/1.0/bpmn">
  
  <process id="FormsProcess" isExecutable="true">
  
    <startEvent id="start" camunda:formKey="deployment:org/camunda/bpm/engine/test/api/form/start.form" />

    <sequenceFlow id="flow1" sourceRef="start" targetRef="task" />
    
    <userTask id="task"
              camunda:formKey="deployment:org/camunda/bpm/engine/test/api/form/task.form"
              camunda:assignee="kermit" />

    <sequenceFlow id="flow2" sourceRef="task" targetRef="wait" />
    
    <receiveTask id="wait" />

  </process>
  
</definitions>
