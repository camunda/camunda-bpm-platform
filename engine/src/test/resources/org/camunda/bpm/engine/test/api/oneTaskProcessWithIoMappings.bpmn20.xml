<?xml version="1.0" encoding="UTF-8"?>
<definitions 
  id="definitions" 
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="ioMappingProcess" name="The One Task Process with io mappings" isExecutable="true">
	<documentation>This is a process for testing purposes</documentation>

    <startEvent id="theStart" />
    <sequenceFlow id="flow1" sourceRef="theStart" targetRef="theTask" />
    <userTask id="theTask" name="my task">
      <extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="inputMappingExecuted">${true}</camunda:inputParameter>
          <camunda:outputParameter name="outputMappingExecuted">${true}</camunda:outputParameter>
        </camunda:inputOutput>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow2" sourceRef="theTask" targetRef="theEnd" />
    <endEvent id="theEnd" />
  </process>

</definitions>