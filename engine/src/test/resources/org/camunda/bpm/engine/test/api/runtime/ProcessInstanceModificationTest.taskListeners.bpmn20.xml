<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions" xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn" targetNamespace="Examples">

  <process id="taskListenerProcess" isExecutable="true">

    <startEvent id="theStart" />
    <sequenceFlow id="flow1" sourceRef="theStart" targetRef="task" />
    <userTask id="task" >
      <extensionElements>
        <camunda:taskListener event="create" delegateExpression="${listener}" />
        <camunda:taskListener event="delete" delegateExpression="${listener}" />
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow3" sourceRef="task" targetRef="theEnd" />
    <endEvent id="theEnd" />
  </process>

</definitions>