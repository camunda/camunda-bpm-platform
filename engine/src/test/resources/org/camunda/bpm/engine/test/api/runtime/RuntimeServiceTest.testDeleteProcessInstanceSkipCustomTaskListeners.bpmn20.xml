<?xml version="1.0" encoding="UTF-8"?>
<definitions
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="oneTaskProcess" name="The One Task Process" isExecutable="true">
    <documentation>This is a process for testing purposes</documentation>
  
    <startEvent id="theStart" />
    <sequenceFlow id="flow1" sourceRef="theStart" targetRef="theTask" />
    <userTask id="theTask" name="my task" camunda:assignee="kermit">
      <extensionElements>
        <camunda:taskListener event="create" class="org.camunda.bpm.engine.test.bpmn.tasklistener.util.RecorderTaskListener" />
        <camunda:taskListener event="assignment" class="org.camunda.bpm.engine.test.bpmn.tasklistener.util.RecorderTaskListener" />
        <camunda:taskListener event="complete" class="org.camunda.bpm.engine.test.bpmn.tasklistener.util.RecorderTaskListener" />
        <camunda:taskListener event="delete" class="org.camunda.bpm.engine.test.bpmn.tasklistener.util.RecorderTaskListener" />
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow2" sourceRef="theTask" targetRef="theEnd" />
    <endEvent id="theEnd" />
    
  </process>

</definitions>
