<?xml version="1.0" encoding="UTF-8"?>
<definitions id="taskAssigneeExample" 
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="Examples">

    <process id="testProcess" isExecutable="true">

        <startEvent id="theStart" />

        <sequenceFlow sourceRef="theStart" targetRef="service1" />

        <serviceTask id="service1" camunda:class="org.camunda.bpm.engine.test.api.runtime.util.SetSerializedVariablesDelegate"></serviceTask>

        <sequenceFlow sourceRef="service1" targetRef="theTask" />

        <userTask id="theTask" />

        <sequenceFlow id="flow2" sourceRef="theTask" targetRef="theEnd" />

        <endEvent id="theEnd" />

    </process>

</definitions>