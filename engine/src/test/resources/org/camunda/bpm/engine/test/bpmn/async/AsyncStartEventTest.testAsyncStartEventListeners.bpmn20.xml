<?xml version="1.0" encoding="UTF-8"?>
<definitions 
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:camundabpm="http://activiti.org/bpmn"
  targetNamespace="Examples">

  <process id="asyncStartEvent">
    <extensionElements>
	    <camundabpm:executionListener event="start"
	      class="org.camunda.bpm.engine.test.bpmn.async.AsyncListener"></camundabpm:executionListener>
	  </extensionElements>
  
    <startEvent id="startEvent" camundabpm:async="true" />
    
    <sequenceFlow sourceRef="startEvent" targetRef="task" />
      
    <userTask id="task"/>
    
    <sequenceFlow sourceRef="task" targetRef="endEvent" />
        
    <endEvent id="endEvent" />
    
  </process>

</definitions>
