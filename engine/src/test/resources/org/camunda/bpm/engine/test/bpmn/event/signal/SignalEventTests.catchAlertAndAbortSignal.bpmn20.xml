<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions"
	xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
	xmlns:camunda="http://camunda.org/schema/1.0/bpmn" 
	targetNamespace="Examples">
	
	<signal id="alertSignal" name="alert" />
	<signal id="abortSignal" name="abort" />

	<process id="catchAlertAndAbortSignal">

		<startEvent id="start" />

		<sequenceFlow sourceRef="start" targetRef="gw1" />

		<eventBasedGateway id="gw1" />
		
		<sequenceFlow sourceRef="gw1" targetRef="catchAlertSignal" />
	    <sequenceFlow sourceRef="gw1" targetRef="catchAbortSignal" />
	
	    <intermediateCatchEvent id="catchAlertSignal" name="Alert">
	      <signalEventDefinition signalRef="alertSignal" />
	    </intermediateCatchEvent>
					
		<sequenceFlow sourceRef="catchAlertSignal" targetRef="end" />
	
	    <intermediateCatchEvent id="catchAbortSignal" name="Abort">
	      <signalEventDefinition signalRef="abortSignal" />
	    </intermediateCatchEvent>

		<sequenceFlow sourceRef="catchAbortSignal" targetRef="end" />

		<endEvent id="end" />

	</process>

</definitions>