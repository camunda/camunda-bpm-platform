<?xml version="1.0" encoding="UTF-8"?>
<definitions
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="intermediateTimerJobPrioProcess" camunda:jobPriority="8" isExecutable="true">
  
    <startEvent id="start" />
    <sequenceFlow id="flow1" sourceRef="start" targetRef="timer1" />
    <intermediateCatchEvent id="timer1">
      <timerEventDefinition>
        <timeDuration>PT1M</timeDuration>
      </timerEventDefinition>
    </intermediateCatchEvent>
    <sequenceFlow id="flow2" sourceRef="timer1" targetRef="timer2" />
    <intermediateCatchEvent id="timer2" camunda:jobPriority="4" >
      <timerEventDefinition>
        <timeDuration>PT1M</timeDuration>
      </timerEventDefinition>
    </intermediateCatchEvent>
    <sequenceFlow id="flow3" sourceRef="timer2" targetRef="end" />
    <endEvent id="end" />
    
  </process>

</definitions>
