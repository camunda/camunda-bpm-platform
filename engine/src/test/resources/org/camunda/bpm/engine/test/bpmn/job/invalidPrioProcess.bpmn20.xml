<?xml version="1.0" encoding="UTF-8"?>
<definitions
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="jobPrioProcess"  isExecutable="true">
  
    <startEvent id="start" />
    <sequenceFlow id="flow1" sourceRef="start" targetRef="task1" />
    <manualTask id="task1" camunda:asyncBefore="true" camunda:asyncAfter="true" />    
    <sequenceFlow id="flow2" sourceRef="task1" targetRef="task2" />
    <manualTask id="task2" camunda:asyncBefore="true" camunda:asyncAfter="true" 
      camunda:jobPriority="thisIsNotANumber" />    
    <sequenceFlow id="flow3" sourceRef="task2" targetRef="end" />
    <endEvent id="end" />
    
  </process>

</definitions>
