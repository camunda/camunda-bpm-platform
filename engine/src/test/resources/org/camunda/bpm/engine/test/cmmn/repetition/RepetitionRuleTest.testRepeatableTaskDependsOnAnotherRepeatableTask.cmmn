<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<definitions id="_7f0c94c0-2a22-445d-b4b7-4fd181e08248"
                  xmlns="http://www.omg.org/spec/CMMN/20151109/MODEL"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns:camunda="http://camunda.org/schema/1.0/cmmn"
                  targetNamespace="Examples">
  <case id="case">

    <casePlanModel id="CasePlanModel_1">

      <planItem id="PI_HumanTask_0" definitionRef="HumanTask_2" />
      <planItem id="PI_HumanTask_1" definitionRef="HumanTask_1">
        <entryCriterion sentryRef="Sentry_1" />
      </planItem>
      <planItem id="PI_HumanTask_2" definitionRef="HumanTask_1">
        <entryCriterion sentryRef="Sentry_2" />
      </planItem>
      
      <sentry id="Sentry_1">
        <planItemOnPart sourceRef="PI_HumanTask_0">
          <standardEvent>complete</standardEvent>
        </planItemOnPart>
      </sentry>      
      
      <sentry id="Sentry_2">
        <planItemOnPart sourceRef="PI_HumanTask_1">
          <standardEvent>complete</standardEvent>
        </planItemOnPart>
      </sentry>

      <humanTask id="HumanTask_1">
        <defaultControl>
          <repetitionRule>
            <condition>${true}</condition>
          </repetitionRule>
        </defaultControl>
      </humanTask>
      <humanTask id="HumanTask_2" />

    </casePlanModel>
  </case>

</definitions>
