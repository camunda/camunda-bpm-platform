<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<definitions id="_7f0c94c0-2a22-445d-b4b7-4fd181e08248"
                  xmlns="http://www.omg.org/spec/CMMN/20131201/MODEL"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns:camunda="http://camunda.org/schema/1.0/cmmn"
                  targetNamespace="Examples">
  <case id="case">

    <casePlanModel id="CasePlanModel_1">

      <planItem id="PI_Stage_1" definitionRef="Stage_1" exitCriteriaRefs="Sentry_2" />
      <planItem id="PI_HumanTask_1" definitionRef="HumanTask_1" exitCriteriaRefs="Sentry_1" />

      <sentry id="Sentry_1">
        <planItemOnPart sourceRef="PI_Stage_1">
          <standardEvent>suspend</standardEvent>
        </planItemOnPart>
      </sentry>

      <sentry id="Sentry_2">
        <planItemOnPart sourceRef="PI_HumanTask_1">
          <standardEvent>exit</standardEvent>
        </planItemOnPart>
      </sentry>

      <humanTask id="HumanTask_1" />

      <humanTask id="Inner_HumanTask_1">
        <extensionElements>
          <camunda:caseExecutionListener class="org.camunda.bpm.engine.test.cmmn.sentry.SentryTriggerListener" event="parentSuspend" />
        </extensionElements>
      </humanTask>

      <stage id="Stage_1">
        <planItem id="PI_Inner_HumanTask_1" definitionRef="Inner_HumanTask_1" />
        <planItem id="PI_Inner_HumanTask_2" definitionRef="Inner_HumanTask_1" exitCriteriaRefs="Inner_Sentry_1" />
        <planItem id="PI_Inner_HumanTask_3" definitionRef="Inner_HumanTask_1" exitCriteriaRefs="Inner_Sentry_2" />

        <sentry id="Inner_Sentry_1">
          <planItemOnPart sourceRef="PI_Inner_HumanTask_1">
            <standardEvent>parentSuspend</standardEvent>
          </planItemOnPart>
        </sentry>

        <sentry id="Inner_Sentry_2">
          <planItemOnPart sourceRef="PI_Inner_HumanTask_2">
            <standardEvent>exit</standardEvent>
          </planItemOnPart>
        </sentry>

      </stage>

    </casePlanModel>
  </case>

</definitions>
