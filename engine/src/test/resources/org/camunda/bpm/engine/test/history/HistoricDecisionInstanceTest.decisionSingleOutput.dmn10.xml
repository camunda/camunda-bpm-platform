    <DecisionTable id="table">
      <clause id="in" name="input">
      <inputExpression id="inputExpression">
        <text>${input1}</text>
      </inputExpression>
        <inputEntry id="input1">
          <text>a</text>
        </inputEntry>
      </clause>
      <clause id="out" name="output" camunda:output="result" >
        <outputEntry id="output1">
          <text>okay</text>
        </outputEntry>
      </clause>
      <rule id="rule">
        <conclusion>output1</conclusion>
      </rule>
    </DecisionTable>
