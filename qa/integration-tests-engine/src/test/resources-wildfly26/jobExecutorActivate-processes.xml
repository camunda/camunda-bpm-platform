<?xml version="1.0" encoding="UTF-8"?>
<process-application xmlns="http://www.camunda.org/schema/1.0/ProcessApplication" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.camunda.org/schema/1.0/ProcessApplication http://www.camunda.org/schema/1.0/ProcessApplication ">

  <process-engine name="jobExecutorActivate-FALSE-engine">
    <job-acquisition>second</job-acquisition>
    <datasource>java:jboss/datasources/ProcessEngine</datasource>
    <properties>
      <property name="history">full</property>
      <property name="jobExecutorActivate">false</property>
    </properties>
  </process-engine>

  <process-engine name="jobExecutorActivate-UNDEFINED-engine">
    <job-acquisition>third</job-acquisition>
    <datasource>java:jboss/datasources/ProcessEngine</datasource>
    <properties>
      <property name="history">full</property>
    </properties>
  </process-engine>

</process-application>