          <text>greeter.greet()</text>
        </inputExpression>
      </input>

      <output id="clause2" label="Check Result" name="result" />

      <rule id="rule1">
        <inputEntry id="inputEntry1">
          <text>"hello"</text>
        </inputEntry>
        <outputEntry id="outputEntry1">
          <text>"ok"</text>
        </outputEntry>
      </rule>

    </decisionTable>
