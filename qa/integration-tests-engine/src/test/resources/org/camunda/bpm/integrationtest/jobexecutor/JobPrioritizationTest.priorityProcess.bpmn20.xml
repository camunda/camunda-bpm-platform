<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions"
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="priorityProcess" camunda:jobPriority="${priorityBean.getPriority()}" isExecutable="true">

    <startEvent id="start" />
    <sequenceFlow sourceRef="start" targetRef="serviceTask" />
    <serviceTask id="serviceTask" camunda:expression="${true}" camunda:asyncBefore="true" />
    <sequenceFlow id="flow1" sourceRef="serviceTask" targetRef="end" />
    <endEvent id="end" />

  </process>
</definitions>
