<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions"
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn" 
  targetNamespace="Examples">


  <process id="userTaskProcess" isExecutable="true">

    <startEvent id="start" />
    <sequenceFlow sourceRef="start" targetRef="userTask" />
    <userTask id="userTask" camunda:jobPriority="${priorityBean.getPriority()}" camunda:asyncAfter="true" />
    <sequenceFlow id="flow1" sourceRef="userTask" targetRef="end" />
    <endEvent id="end" />

  </process>
</definitions>
