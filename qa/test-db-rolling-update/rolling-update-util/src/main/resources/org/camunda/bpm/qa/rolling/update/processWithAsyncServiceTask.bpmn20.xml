<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions"
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:camunda="http://activiti.org/bpmn"
  targetNamespace="Examples">
  <process id="processWithAsyncServiceTask" isExecutable="true">
    <startEvent id="theStart" />
    <sequenceFlow id="flow1" sourceRef="theStart" targetRef="userTask" />
    <serviceTask id="userTask" camunda:asyncBefore="true" camunda:expression="${true}" />
    <sequenceFlow id="flow2" sourceRef="userTask" targetRef="end" />
    <endEvent id="end" />
  </process>
</definitions>
