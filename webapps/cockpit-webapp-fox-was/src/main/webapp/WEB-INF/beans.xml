<?xml version="1.0"?>
<beans xmlns="http://java.sun.com/xml/ns/javaee"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://java.sun.com/xml/ns/javaee  http://docs.jboss.org/cdi/beans_1_0.xsd">
  
  <alternatives>
		<!-- generates some demo data for development purposes -->
    <class>com.camunda.fox.cockpit.webapp.demo.DemoDataGenerator</class>
    <class>com.camunda.fox.cockpit.demo.deployer.FoxPlatformDemoDataDeployer</class>
  </alternatives>
  
  <!-- disabled because OpenWebBeans only needs/allows the definition of an
    interceptor in ONE beans.xml, in contrary to Weld, where you must enable
    an interceptor in the containing jar and the war too.
  <interceptors>
    <class>com.camunda.fox.cdi.transaction.interceptor.TransactionInterceptor</class>
    <class>com.camunda.fox.security.interceptor.SecurityInterceptor</class>
  </interceptors>
  -->
</beans>